{
  "workbench.iconTheme": "material-icon-theme",
  "editor.linkedEditing": true,
  "editor.formatOnSave": true,
  "diffEditor.wordWrap": "on",
  "editor.fontFamily": "Fira Code iScript, Fira Code, Operator Mono",
  "editor.fontLigatures": true,
  "editor.fontWeight": 500,
  "emmet.triggerExpansionOnTab": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },

  "semi": false,
  "singleQuote": true,

  "rainbowTags.colors": [
    "#FBFBFB",
    "#8cd9e3",
    "#90ee90",
    "#ff6347",
    "#00bfff",
    "#ffa07a",
    "#f0e68c",
    "#d87093",
    "#14A44D",
    "#dc143c",
    "#FF8283",
    "#0dcaf0",
    "#5b82a1",
    "#32cd32",
    "#F5A25C",
    "#20c997",
    "#FBFBFB",
    "#fd7e14"
  ],
  "rainbowTags.supportedLanguages": ["html", "xml", "react"],
  "rainbowTags.hightlightType": "color",
  "rainbowTags.allowEverywhere": true,
  "rainbowTags.denylistTags": ["html", "head", "body"],
  "colorful-comments.tags": [
    {
      "tag": "?",
      "color": "#f4a460",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "^",
      "color": "#d87093",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "******",
      "color": "#90ee90",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "&",
      "color": "#ff6347",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "~",
      "color": "#00bfff",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "#",
      "color": "#f0e68c",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "@",
      "color": "#fffafa",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "%",
      "color": "#32cd32",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "liveServer.settings.donotShowInfoMsg": true,
  "prettier.semi": false,
  "javascript.format.insertSpaceAfterSemicolonInForStatements": false,
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.lineHeight": 1.1,
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.fontWeight": 500,
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "thunder-client.saveToWorkspace": true,
  "liveServer.settings.donotVerifyTags": true,
  "headwind.runOnSave": false,
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/src/styles"
    }
  ],
  "[python]": {
    "editor.defaultFormatter": null,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "editor.formatOnType": true
  },
  "workbench.colorCustomizations": {
    "tab.activeBorder": "#FE60C2",
    "badge.background": "#FE60C2",
    "badge.foreground": "#fff",
    "activityBarBadge.background": "#FE60C2",
    "activityBarBadge.foreground": "#fff",
    "editor.background": "#13141b",
    "sideBar.background": "#0f1017",
    "activityBar.background": "#161723",
    "statusBar.background": "#0c0c13",
    "titleBar.activeBackground": "#1D1E27",
    "menu.background": "#161723",
    "tab.inactiveBackground": "#1b1d28",
    "tab.hoverBackground": "#0c0c13",
    "editorGroupHeader.tabsBackground": "#191c25",
    "gitDecoration.modifiedResourceForeground": "#e2e2e3",
    "input.background": "#11121b",
    "scrollbarSlider.hoverBackground": "#272737",
    "scrollbarSlider.activeBackground": "#272737",
    "scrollbarSlider.background": "#272737"
  },
  "editor.tokenColorCustomizations": {
    "comments": "#777980",
    "functions": "#00D075",
    "strings": "#ffef8b",
    "numbers": "#ff9e37",
    "textMateRules": [
      {
        "scope": ["storage.type.js"],
        "settings": {
          "foreground": "#FE64C6"
        }
      },
      {
        "scope": ["variable.parameter.js"],
        "settings": {
          "foreground": "#AF86F8"
        }
      },
      {
        "scope": [
          "variable.other.constant.js",
          "string.quoted.single.js",
          "variable.other.object.js"
        ],
        "settings": {
          "foreground": "#edeeff"
        }
      },
      {
        "scope": [
          "keyword.control.import.js",
          "keyword.control.from.js",
          "keyword.control.as.js",
          "keyword.control.flow.js"
        ],
        "settings": {
          "foreground": "#FC5FB6"
        }
      }
    ]
  },
  "window.zoomLevel": 1
}
