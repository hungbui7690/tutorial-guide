  "editor.tokenColorCustomizations": {
    "comments": "#626a88",
    // "functions": "#00D075",
    "functions": "#6ee7b7",
    "strings": "#ffefb0",
    // "numbers": "#ff9e37",
    "numbers": "#f43f5e",

    "textMateRules": [
      {
        "scope": [
          "storage.type.js",
          "storage.type.ts",
          "entity.other.attribute-name.html",
          "storage.type.tsx"
        ],
        "settings": {
          "foreground": "#8192a7"
        }
      },
      {
        "scope": [
          "variable.parameter.js",
          "variable.parameter.ts",
          "variable.parameter.tsx"
        ],
        "settings": {
          "foreground": "#AF86F8"
        }
      },
      {
        "scope": [
          "variable.other.constant.js",
          "variable.other.object.js",
          "string.quoted.single.js",
          "string.quoted.double.html",
          "variable.other.constant.ts",
          "variable.other.object.ts",
          "string.quoted.single.ts",
          "variable.other.constant.tsx",
          "variable.other.object.tsx",
          "string.quoted.single.tsx"
        ],
        "settings": {
          "foreground": "#c4c6e9"
        }
      },
      {
        "scope": [
          "meta.block.js.jsx",
          "meta.block.js.jsx",
          "meta.function.js.jsx",
          "meta.block.js.jsx",
          "meta.function.expression.js.jsx",
          "meta.block.js.jsx",
          "meta.function.expression.js.jsx",
          "meta.export.default.js.jsx",
          "source.js.jsx"
        ],
        "settings": {
          "foreground": "#43a0c8"
        }
      },
      {
        "scope": [
          "keyword.control.import.js",
          "keyword.control.from.js",
          "keyword.control.as.js",
          "keyword.control.export.js",
          "keyword.operator.assignment.ts",
          "keyword.control.import.ts",
          "keyword.control.from.ts",
          "keyword.control.as.ts",
          "keyword.control.export.ts",
          "keyword.operator.assignment.tsx",
          "keyword.control.import.tsx",
          "keyword.control.from.tsx",
          "keyword.control.as.tsx",
          "keyword.control.export.tsx"
        ],
        "settings": {
          "foreground": "#ff2ca0"
        }
      },
      {
        "scope": [
          "keyword.control.conditional.js",
          "keyword.control.conditional.ts",
          "keyword.control.conditional.tsx"
        ],
        "settings": {
          "foreground": "#AF86F8"
        }
      },
      {
        "scope": [
          "variable.parameter.js",
          "variable.parameter.ts",
          "variable.parameter.tsx"
        ],
        "settings": {
          "foreground": "#f68191"
        }
      },
      {
        "scope": [
          "variable.other.object.js",
          "variable.other.object.tsx",
          "variable.other.object.ts"
        ],
        "settings": {
          "foreground": "#70a18e"
        }
      },
      {
        "scope": [
          "string.quoted.double.json",
          "string.quoted.double.js",
          "meta.jsx.children.js",
          "string.quoted.double.ts",
          "meta.jsx.children.ts",
          "string.quoted.double.tsx",
          "meta.jsx.children.tsx"
        ],
        "settings": {
          "foreground": "#95FFA4"
        }
      },
      {
        "scope": [
          "support.type.property-name.json",
          "storage.type.function.js",
          "storage.type.function.tsx"
        ],
        "settings": {
          "foreground": "#82b7c9"
        }
      },
      {
        "scope": ["constant.language.json"],
        "settings": {
          "foreground": "#AF86F8"
        }
      },
      {
        "scope": [],
        "settings": {
          "foreground": "#d6beff"
        }
      },
      {
        "scope": [
          "keyword.control.loop.js",
          "keyword.control.loop.ts",
          "keyword.control.loop.tsx"
        ],
        "settings": {
          "foreground": "#9895E7"
        }
      },

      {
        "scope": [],
        "settings": {
          "foreground": "#f43f5e"
        }
      },
      {
        "scope": [
          "keyword.operator.new.js",
          "entity.other.inherited-class.js",
          "keyword.operator.new.ts",
          "entity.other.inherited-class.ts",
          "keyword.operator.new.tsx",
          "entity.other.inherited-class.tsx"
        ],
        "settings": {
          "foreground": "#FF8C00"
        }
      },
      {
        "scope": [
          "entity.name.type.class.js",
          "entity.name.type.class.ts",
          "entity.name.type.class.tsx"
        ],
        "settings": {
          "foreground": "#95FFA4"
        }
      },
      {
        "scope": [
          "entity.other.attribute-name.js",
          "entity.other.attribute-name.ts",
          "entity.other.attribute-name.tsx",
          "string.quoted.docstring.multi.python"
        ],
        "settings": {
          "foreground": "#94a3b8"
        }
      },
      {
        "scope": [
          "keyword.operator.assignment.js",
          "meta.brace.round.js",
          "punctuation.definition.block.js",
          "keyword.operator.assignment.ts",
          "meta.brace.round.ts",
          "punctuation.definition.block.ts",
          "keyword.operator.assignment.tsx",
          "meta.brace.round.tsx",
          "punctuation.definition.block.tsx"
        ],
        "settings": {
          "foreground": "#DC1345",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "storage.modifier.async.js",
          "keyword.control.default.js",
          "keyword.control.flow.js",
          "keyword.control.trycatch.js",
          "storage.modifier.async.ts",
          "keyword.control.default.ts",
          "keyword.control.flow.ts",
          "keyword.control.trycatch.ts",
          "storage.modifier.async.tsx",
          "keyword.control.default.tsx",
          "keyword.control.flow.tsx",
          "keyword.control.trycatch.tsx"
        ],
        "settings": {
          "foreground": "#F72585"
        }
      },
      {
        "scope": [],
        "settings": {
          "foreground": "#CA547F"
        }
      },
      {
        "scope": [],
        "settings": {
          "foreground": "#d65076"
        }
      },
      {
        "scope": [
          "punctuation.definition.arguments.begin.python",
          "meta.function-call.python"
        ],
        "settings": {
          "foreground": "#45B8AC"
        }
      },
      {
        "scope": [],
        "settings": {
          "foreground": "#5B5EA6"
        }
      },
      {
        "scope": [],
        "settings": {
          "foreground": "#9B2335"
        }
      },
      {
        "scope": [],
        "settings": {
          "foreground": "#C3447A"
        }
      },
      {
        "scope": [],
        "settings": {
          "foreground": "#98B4D4"
        }
      },

      {
        "scope": [],
        "settings": {
          "foreground": "#AF8829"
        }
      }
    ]
  },
