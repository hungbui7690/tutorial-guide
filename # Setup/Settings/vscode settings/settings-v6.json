{
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.formatOnSave": true,
  "diffEditor.wordWrap": "on",
  "editor.fontFamily": "Fira Code iScript, Fira Code, Operator Mono",
  "editor.fontLigatures": true,
  "editor.fontWeight": 500,
  "emmet.triggerExpansionOnTab": true,
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.formatOnSave": false,
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 80
  },
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "semi": false,
  "singleQuote": true,
  "colorful-comments.tags": [
    {
      "tag": "??",
      "color": "#f4a460",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "^^",
      "color": "#d87093",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "''''''''''''''''''",
      "color": "#90ee90",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "&&",
      "color": "#ff6347",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "~~",
      "color": "#00bfff",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "##",
      "color": "#f0e68c",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "@@",
      "color": "#E05383",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "%%",
      "color": "#32cd32",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "!!",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "liveServer.settings.donotShowInfoMsg": true,
  "prettier.semi": false,
  "javascript.format.insertSpaceAfterSemicolonInForStatements": false,
  "terminal.integrated.fontFamily": "MesloLGL Nerd Font",
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.fontWeight": 500,
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "security.workspace.trust.untrustedFiles": "open",
  "security.workspace.trust.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "thunder-client.saveToWorkspace": true,
  "liveServer.settings.donotVerifyTags": true,
  "headwind.runOnSave": false,
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.lineHeight": 1.4,
  "editor.letterSpacing": ".5",
  "rainbowTags.supportedLanguages": ["html", "xml", "jsx"],
  "rainbowTags.hightlightType": "color",
  "rainbowTags.allowEverywhere": true,
  "rainbowTags.denylistTags": [
    "html",
    "head",
    "body",
    "meta",
    "title",
    "input",
    "BrowserRouter",
    "Routes",
    "Route",
    "Box"
